<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义属性 -->
    <declare-styleable name="ClipViewLayout">
        <attr name="clipType" format="enum">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
        </attr>
        <attr name="mHorizontalPadding" format="dimension" />
        <attr name="clipBorderWidth" format="dimension" />
    </declare-styleable>


    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="YSInputLayout">
        <attr name="text_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="YSToolBar">
        <attr name="ystoolbar_middle_text_view_text" format="string" />
        <attr name="ystoolbar_right_button_text" format="string" />
    </declare-styleable>

    <declare-styleable name="YSToolBar2">
        <attr name="ystoolbar2_left_button_text" format="string" />
        <attr name="ystoolbar2_middle_text_view_text" format="string" />
        <attr name="ystoolbar2_right_button_text" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutVertical">
        <attr name="title_text" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutVerticalImage">
        <attr name="top_text_view_text" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutHorizontalSingleTextView">
        <attr name="title_text_h" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutHorizontalDoubleTextView">
        <attr name="left_text_view_text" format="string" />
        <attr name="right_text_view_text" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutHorizontalWeiDoubleTextView">
        <attr name="wei_left_text_view_text" format="string" />
        <attr name="wei_right_text_view_text" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutHorizontalTextViewEditText">
        <attr name="text_view_text" format="string" />
        <attr name="right_edit_text_text" format="string" />
        <attr name="right_edit_text_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutHorizontalTextViewEditText2">
        <attr name="text_view_text2" format="string" />
        <attr name="right_edit_text_text2" format="string" />
        <attr name="right_edit_text_hint2" format="string" />
    </declare-styleable>

    <declare-styleable name="YSSearchView">
        <attr format="string" name="sc_corner_radius"/>
        <attr format="dimension" name="sc_border_width"/>
        <attr format="string" name="sc_tint_color"/>
        <attr format="color" name="sc_checked_text_color"/>
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutVerticalTextViewButtonEditText">
        <attr name="ys_text_view_text" format="string" />
        <attr name="ys_button_text" format="string" />
        <attr name="ys_button_visible" format="boolean" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutVerticalDoubleTextView">
        <attr name="ys_top_text_view_text" format="string" />
        <attr name="bottom_text_view_text" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutVerticalTextViewEditText">
        <attr name="ys_vertical_top_text_view_text" format="string" />
        <attr name="ys_vertical_bottom_edit_text_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutHorizontalDoubleTextView2">
        <attr name="left_text_view_text2" format="string" />
        <attr name="right_text_view_text2" format="string" />
    </declare-styleable>

    <declare-styleable name="YSLinearLayoutHorizontalDoubleTextView3">
        <attr name="left_text_view_text3" format="string" />
        <attr name="right_text_view_text3" format="string" />
    </declare-styleable>

</resources>