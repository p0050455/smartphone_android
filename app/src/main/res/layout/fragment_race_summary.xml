<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="比赛名称"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_organization_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="举办单位"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_caipanzhang"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="裁判长"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_caipanyuan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="裁判员"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_jingweidu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="经纬度"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_jiegeshijian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="集鸽时间"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_sifangshijian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="司放时间"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_fangfeitianqi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="放飞天气"/>

    <com.pactera.widget.YSLinearLayoutHorizontalDoubleTextView
        android:id="@+id/fragment_race_summary_text_view_cankaokongju"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:left_text_view_text="参考空距"/>


</LinearLayout>